  &lt;label class="select__label" for="example4b"&gt;
    &lt;span class="select__content hidden"&gt;Hidden select&lt;/span&gt;
    &lt;select class="select__control" id="example4b" name="example4b"&gt;
        &lt;option value=""&gt;Choose an option&lt;/option&gt;
        &lt;option value="one"&gt;one&lt;/option&gt;
        &lt;option value="two"&gt;two&lt;/option&gt;
        &lt;option value="three"&gt;three&lt;/option&gt;
    &lt;/select&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- Select with hint --&gt;
&lt;div class="select"&gt;
  &lt;label class="select__label" for="example4b"&gt;
    &lt;span class="select__content"&gt;Select with hint message&lt;/span&gt;
    &lt;select class="select__control" id="example4b" name="example4b"&gt;
        &lt;option value=""&gt;Choose an option&lt;/option&gt;
        &lt;option value="one"&gt;one&lt;/option&gt;
        &lt;option value="two"&gt;two&lt;/option&gt;
        &lt;option value="three"&gt;three&lt;/option&gt;
    &lt;/select&gt;
    &lt;span class="message-hint"&gt;Hint Message&lt;/span&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- Required select --&gt;
&lt;div class="select select--required"&gt;
  &lt;label class="select__label" for="example4c"&gt;
    &lt;span class="select__content"&gt;Required select&lt;/span&gt;
    &lt;select class="select__control" id="example4c" name="example4c" required&gt;
        &lt;option value=""&gt;Choose an option&lt;/option&gt;
        &lt;option value="one"&gt;one&lt;/option&gt;
        &lt;option value="two"&gt;two&lt;/option&gt;
        &lt;option value="three"&gt;three&lt;/option&gt;
    &lt;/select&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- Invalid select --&gt;
&lt;div class="select isInvalid"&gt;
  &lt;label class="select__label" for="example4d"&gt;
    &lt;span class="select__content"&gt;Inalid select&lt;/span&gt;
    &lt;select class="select__control" id="example4d" name="example4d"&gt;
        &lt;option value=""&gt;Choose an option&lt;/option&gt;
        &lt;option value="one"&gt;one&lt;/option&gt;
        &lt;option value="two"&gt;two&lt;/option&gt;
        &lt;option value="three"&gt;three&lt;/option&gt;
    &lt;/select&gt;
    &lt;span class="message-error"&gt;Error Message&lt;/span&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- Disabled select --&gt;
&lt;div class="select select--disabled"&gt;
  &lt;label class="select__label" for="example4b"&gt;
