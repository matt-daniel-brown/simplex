      <h2 id="form">form modules</h2>
      <h3>input</h3>
      <h5 class="example-heading">Example</h5>
      <div class="example">
  <!-- @import "modules/input" -->
  <!-- @import "modules/input-placeholder" -->
  <!-- @import "modules/input-value" -->
  <!-- @import "modules/input-hidden" -->
  <!-- @import "modules/input-hint" -->
  <!-- @import "modules/input-required" -->
  <!-- @import "modules/input-invalid" -->
  <!-- @import "modules/input-disabled" -->
      </div>
      <h5 class="example-heading">Markup</h5>
<pre><code class="language-html">&lt;!-- input --&gt;
&lt;div class="input"&gt;
  &lt;label class="input__label" for="example1a"&gt;
    &lt;span class="input__content"&gt;Input&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1a" id="example1a"&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- input with placeholder --&gt;
&lt;div class="input"&gt;
  &lt;label class="input__label" for="example1b"&gt;
    &lt;span class="input__content"&gt;Input with placeholder&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1b" id="example1b" placeholder="Add Your Name"&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- input with value --&gt;
&lt;div class="input"&gt;
  &lt;label class="input__label" for="example1g"&gt;
    &lt;span class="input__content"&gt;Input with value&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1g" id="example1b" value="Add Your Name"&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- input with hidden label --&gt;
&lt;div class="input"&gt;
  &lt;label class="input__label" for="example1h"&gt;
    &lt;span class="input__content hidden"&gt;Hidden input&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1h" id="example1b" value="Add Your Name"&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- input with hint --&gt;
&lt;div class="input"&gt;
  &lt;label class="input__label" for="example1f"&gt;
    &lt;span class="input__content"&gt;Input with hint message&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1f" id="example1f"&gt;
    &lt;span class="message-hint"&gt;Hint Message&lt;/span&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- input required --&gt;
&lt;div class="input input--required"&gt;
  &lt;label class="input__label" for="example1e"&gt;
    &lt;span class="input__content"&gt;Input required&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1e" id="example1e" required&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- input invalid --&gt;
&lt;div class="input isInvalid"&gt;
  &lt;label class="input__label" for="example1f"&gt;
    &lt;span class="input__content"&gt;Input invalid&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1f" id="example1f" required&gt;
    &lt;span class="message-error"&gt;Error Message&lt;/span&gt;
  &lt;/label&gt;
&lt;/div&gt;

&lt;!-- input disabled --&gt;
&lt;div class="input input--disabled"&gt;
  &lt;label class="input__label" for="example1d"&gt;
    &lt;span class="input__content"&gt;Input disabled&lt;/span&gt;
    &lt;input class="input__control" type="text" name="example1d" id="example1d" disabled&gt;
  &lt;/label&gt;
&lt;/div&gt;
</code></pre>

