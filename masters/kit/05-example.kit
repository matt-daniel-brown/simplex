  <div class="row-feature">
    <div class="container"></div>
    <div class="container-narrow center">
      <h2 class="mar-t-0">Contact us</h2>
      <form action="#">

        <div class="row">
          <div class="col-sm-7">

            <div class="input input--required">
              <label class="input__label" for="name">
                <span class="input__content">Name</span>
                <input class="input__control" type="text" name="name" id="name" required>
              </label>
            </div>

            <div class="input input--required">
              <label class="input__label" for="email">
                <span class="input__content">Email</span>
                <input class="input__control" type="email" email="email" id="email" required>
              </label>
            </div>

            <div class="select select--required">
              <label class="select__label" for="message-type">
                <span class="select__content">Enquiry type</span>
                <select class="select__control" id="message-type" name="message-type" required>
                    <option value="">Choose an option</option>
                    <option value="Enquiry">Enquiry</option>
                    <option value="Complaint">Complaint</option>
                    <option value="Comment">Comment</option>
                </select>
              </label>
            </div>

          </div>
        </div>

        <div class="textarea">
          <label class="textarea__label" for="comment">
            <span class="textarea__content">Comment</span>
            <textarea class="textarea__control" id="comment" rows="5"></textarea>
          </label>
        </div>

        <button type="submit" class="button-primary">Submit</button>
      </form>
    </div>
